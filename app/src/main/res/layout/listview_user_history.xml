<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/card_view"
    style="@style/CardViewStyle"
    android:layout_width="match_parent"
    android:layout_height="450dp"
    android:layout_alignParentTop="true"
    android:layout_centerVertical="true"
    android:layout_margin="20dp">

 <RelativeLayout
     android:id="@+id/relativeLayout_main"
     android:layout_width="match_parent"
     android:layout_height="match_parent"
     android:elevation="5dp" android:layout_margin="5dp">

  <RelativeLayout
      android:id="@+id/relativeLayout_main2"
      android:layout_width="match_parent"
      android:layout_height="wrap_content">

   <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:id="@+id/textView_requestid"
       android:textSize="16sp"
       android:textColor="#000000"
       android:fontFamily="monospace"
       android:layout_alignBottom="@+id/textView7"
       android:layout_toEndOf="@+id/textView7"
       android:layout_toRightOf="@+id/textView7"
       android:layout_marginLeft="5dp"/>

   <TextView
       android:text="Request Number :"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:textSize="16sp"
       android:fontFamily="monospace"
       android:textColor="#000000"
       android:id="@+id/textView7"
       android:layout_marginTop="10dp"/>


   <TextView
       android:text="Age :"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:textSize="16sp"
       android:textColor="#000000"
       android:fontFamily="monospace"
       android:layout_below="@+id/textView7"
       android:id="@+id/textView88"
       android:layout_marginTop="5dp"/>


   <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:id="@+id/textView_age"
       android:textSize="16sp"
       android:textColor="#000000"
       android:fontFamily="monospace"
       android:layout_alignBottom="@+id/textView88"
       android:layout_toEndOf="@+id/textView88"
       android:layout_toRightOf="@+id/textView88"
       android:layout_marginLeft="5dp"/>


   <TextView
       android:text="Gender :"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:textSize="16sp"
       android:textColor="#000000"
       android:fontFamily="monospace"
       android:layout_below="@+id/textView88"
       android:id="@+id/textView8"
       android:layout_marginTop="5dp"/>


   <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:id="@+id/textView_gender"
       android:textSize="16sp"
       android:textColor="#000000"
       android:fontFamily="monospace"
       android:layout_alignBottom="@+id/textView8"
       android:layout_toEndOf="@+id/textView8"
       android:layout_toRightOf="@+id/textView8"
       android:layout_marginLeft="5dp"/>


   <TextView
       android:text="Weight :"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:textSize="16sp"
       android:textColor="#000000"
       android:fontFamily="monospace"
       android:layout_below="@+id/textView8"
       android:id="@+id/textView9"
       android:layout_marginTop="5dp"/>

   <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:id="@+id/textView_weight"
       android:textSize="16sp"
       android:textColor="#000000"
       android:fontFamily="monospace"
       android:layout_alignBottom="@+id/textView9"
       android:layout_toEndOf="@+id/textView9"
       android:layout_toRightOf="@+id/textView9"
       android:layout_marginLeft="5dp"/>


   <TextView
       android:text="Height :"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:textSize="16sp"
       android:textColor="#000000"
       android:fontFamily="monospace"
       android:layout_alignBottom="@+id/textView_weight"
       android:layout_toEndOf="@+id/textView_weight"
       android:layout_toRightOf="@+id/textView_weight"
       android:id="@+id/textView10"
       android:layout_marginTop="5dp" android:layout_marginLeft="5dp"/>


   <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:id="@+id/textView_height"
       android:textSize="16sp"
       android:textColor="#000000"
       android:fontFamily="monospace"
       android:layout_alignBottom="@+id/textView10"
       android:layout_toEndOf="@+id/textView10"
       android:layout_toRightOf="@+id/textView10"
       android:layout_marginLeft="5dp"/>


   <TextView
       android:text="Waist :"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:textSize="16sp"
       android:textColor="#000000"
       android:fontFamily="monospace"
       android:layout_below="@+id/textView9"
       android:id="@+id/textView11"
       android:layout_marginTop="5dp"/>


   <TextView

       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:id="@+id/textView_waist"
       android:textSize="16sp"
       android:fontFamily="monospace"
       android:textColor="#000000"
       android:layout_alignBottom="@+id/textView11"
       android:layout_toEndOf="@+id/textView11"
       android:layout_toRightOf="@+id/textView11"
       android:layout_marginLeft="5dp"/>


   <TextView
       android:text="Diet Type :"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:textSize="16sp"
       android:fontFamily="monospace"
       android:textColor="#000000"
       android:layout_below="@+id/textView11"
       android:id="@+id/textView12"
       android:layout_marginTop="5dp"/>

   <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:id="@+id/textView_type"
       android:textSize="16sp"
       android:textColor="#000000"
       android:fontFamily="monospace"
       android:layout_alignBottom="@+id/textView12"
       android:layout_toEndOf="@+id/textView12"
       android:layout_toRightOf="@+id/textView12"
       android:layout_marginLeft="5dp"/>


   <TextView
       android:text="Date :"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:textSize="16sp"
       android:fontFamily="monospace"
       android:textColor="#000000"
       android:layout_below="@+id/textView12"
       android:id="@+id/textView13"
       android:layout_marginTop="5dp"/>


   <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:id="@+id/textView_date"
       android:textSize="18sp"
       android:fontFamily="monospace"

       android:textColor="#000000"
       android:layout_alignBottom="@+id/textView13"
       android:layout_toEndOf="@+id/textView13"
       android:layout_toRightOf="@+id/textView13"
       android:layout_marginLeft="5dp"/>


  </RelativeLayout>


  <TextView
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:textSize="16sp"
      android:fontFamily="monospace"
      android:id="@+id/textView_error"
      android:layout_marginTop="100dp"
      android:textColor="#000000"
      android:text="No Plan Submitted"
      android:layout_centerHorizontal="true"
      android:layout_below="@+id/relativeLayout_main2"
      android:layout_marginLeft="5dp"/>

  <WebView
      android:id="@+id/webview_history"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:layout_marginTop="10dp"
      android:layout_below="@+id/relativeLayout_main2"/>


 </RelativeLayout>


</androidx.cardview.widget.CardView>


